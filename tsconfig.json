{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "ignoreDeprecations": "5.0",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": []
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}